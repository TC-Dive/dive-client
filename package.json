{
  "name": "dive",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@7.12.1",
  "scripts": {
    "dev": "pnpm codegen && vite",
    "build": "pnpm codegen && tsc && vite build",
    "preview": "vite preview",
    "lint": "pnpm codegen && eslint app --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint app --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "pnpm codegen && tsc",
    "stylelint": "stylelint --cache --fix --ignore-path .gitignore **/*.css",
    "test": "vitest",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.yml dotenv_config_path=./.env.local",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/core": "^7.20.5",
    "@emotion/react": "^11.10.5",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@mantine/core": "^5.10.0",
    "@mantine/form": "^5.9.4",
    "@mantine/hooks": "^5.10.0",
    "@togglecorp/fujs": "^2.0.0",
    "@urql/devtools": "^2.0.3",
    "@urql/exchange-multipart-fetch": "^1.0.1",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.4.5",
    "urql": "^3.0.3",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@graphql-codegen/cli": "2.16.1",
    "@graphql-codegen/client-preset": "1.2.3",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/schema-ast": "^2.6.0",
    "@graphql-eslint/eslint-plugin": "^3.13.1",
    "@types/node": "^18.11.14",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@urql/core": "^3.1.1",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "browserslist": "^4.21.4",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "pnpm": "^7.28.0",
    "postcss": "^8.4.20",
    "postcss-nested": "^6.0.0",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^8.0.1",
    "stylelint": "^14.16.0",
    "stylelint-config-concentric-order": "^5.1.0",
    "stylelint-config-recommended": "^9.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "typescript-plugin-css-modules": "^4.1.1",
    "vite": "^4.0.1",
    "vite-plugin-eslint": "^1.8.1",
